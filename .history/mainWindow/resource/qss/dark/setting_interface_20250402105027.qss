SettingInterface,
#scrollWidget {
    background-color: rgb(32, 32, 32); /* 比导航栏略亮的深灰色 */
}

QScrollArea {
    border: none;
    background-color: rgb(32, 32, 32);
}

/* 标签 */
QLabel#settingLabel {
    font: 33px 'Microsoft YaHei Light';
    background-color: transparent;
    color: white;
}

/* 卡片样式改进 */
SettingCardGroup {
    background-color: rgb(43, 43, 43);
    border: 1px solid rgb(53, 53, 53);
    border-radius: 6px;
}